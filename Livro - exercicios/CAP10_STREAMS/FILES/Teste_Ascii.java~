import java.lang.*;

public class Teste_Ascii {

// teste


public static void main(String args[]) {
  
  System.out.println("---  TESTE DIABÓLICO ---");
 
  String s1,s2, s3, s4;
  s1 = "Bill Gates";
  s2 = "Windows 95";
  s3 = "World Wide Web";
  s4 = "MS-DOS 6.21";

  int conta = 0;

  for(i=0; s1.length();i++) {
       conta = conta + (int) s1.charAt(i);
  }
  System.out.println("s1 = ", conta);





}
}
