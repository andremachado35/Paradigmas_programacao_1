    public class SubA extends Super {
	   private int x = 20; // “shadow”
	   String nome;   // “shadow”
	//
	   public int getX() { return x; }
	   public String classe() { return "Sub"; }
	   public String supClasse() { return super.classe(); }
	   public int soma() { return x + super.x; }
	}
