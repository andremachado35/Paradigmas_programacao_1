
/**
 * Classe Cao
 * 
 * @author PPIV
 * @version 1.0 (Abril 2005)
 */

public class Cao extends Mamifero {

    /**
     * Construtor por omissão
     */
    public Cao() {
		super("", "canídeo", 0);
	}
	
	/**
	 * Construtor completo
	 */
	public Cao(String nom, String espec, int pesa) {
		super(nom, espec, pesa);
	}
	
	// Métodos de Instância não herdados

	
	/**
	 * Clone()
	 */
	public Object clone() { 
	    return new Cao(this.daNome(), this.daEspecie(),
	                   this.daPeso()); 
    }	
}
